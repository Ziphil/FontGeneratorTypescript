{
  "name": "font-generator",
  "version": "1.0.0",
  "author": "Ziphil",
  "description": "",
  "homepage": "https://github.com/Ziphil/TypescriptFontGenerator",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "develop": "webpack-dev-server --mode development --devtool inline-source-map --config webpack.ts --hot",
    "generate": "ts-node ./source/index.ts"
  },
  "eslintConfig": {
    "extends": "ziphil",
    "ignorePatterns": "webpack.ts",
    "rules": {
      "@typescript-eslint/unified-signatures": "off"
    }
  },
  "nodemonConfig": {
    "watch": [
      "source"
    ],
    "ext": "ts",
    "exec": "ts-node ./source/index.ts"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.0.0",
    "@types/jsdom": "^16.2.6",
    "@types/mini-css-extract-plugin": "^1.2.2",
    "@types/node": "^14.14.32",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "eslint": "^7.15.0",
    "eslint-config-ziphil": "^1.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.8",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "babel-loader": "^8.2.2",
    "babel-polyfill": "^6.26.0",
    "command-line-args": "^5.1.1",
    "css-loader": "^3.2.0",
    "execa": "^5.0.0",
    "html-webpack-plugin": "^4.5.0",
    "jsdom": "^16.5.0",
    "mini-css-extract-plugin": "^1.3.9",
    "nodemon": "^2.0.7",
    "paper": "^0.12.11",
    "query-string": "^6.14.1",
    "reflect-metadata": "^0.1.13",
    "source-map-loader": "^1.1.3",
    "ts-loader": "^8.0.17",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
